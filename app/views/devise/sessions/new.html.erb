      <div class="emailon input-field col m4 offset-m1">
        <%= f.label :email %><br />
        <%= f.email_field :email, autofocus: true %>
      </div>
      <div class="passon input-field col m4 offset-m1">
        <%= f.label :password %><br />
        <%= f.password_field :password, autocomplete: "off" %>
      </div>
        <div class="row">
          <button class="btn signin cyan waves-effect buttonvw"><%= f.submit "Log in" %></button>
          <div class="myclass">
            <button class="btn cyan signup waves-effect buttonvw"><%= link_to "Sign up", new_registration_path(resource_name) %></button>
        </div>
      </div>
    <% end -%>
